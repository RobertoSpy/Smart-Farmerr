struct piesa
{
  int lungime;
  int latime;
  int nume;
};

vector<piesa> piese
{
  {1,5,-1},
  {3,1,-1},
  {4,2,-1}
};
void afisareMatrice(int a[101][101])
{
  for(int i=0;i<7;i++)
  {
    for(int j=0;j<9;j++)
      cout<<a[i][j]<<" ";
      cout<<endl;
  }

}
bool verifgard(int i, int j)
{
  if(i%2==0 && j%2==0)return true;
  return false;
}


int main()
{
   const int nrLinii = 7;
    const int nrColoane = 9;
  
    citire();
    bordarematrice();
    for(int i=0;i<7;i++)
    {
     for(int j=0;j<9;j++)    
       cout<<a[i][j]<<" ";
       cout<<endl;
    }
     int k=0, l=0;
    for (const auto& piesa : piese) 
        while (l<3) 
        {
        
            
                
                int linie, coloana;
                char orientare;
                do{
                  
                cout << "Introduceti linia pentru P" << piesa.nume << ": ";
                cin >> linie;
                
                  
                  
                cout << "Introduceti coloana pentru P" << piesa.nume << ": ";
                cin >> coloana;
                                
                  
                
                if(verifgard(linie, coloana)==false)cout<<"NU PUTEM PUNE GARD AICI, VA ROG REINCERCATI"<<" ";
                

                }while(verifgard(linie, coloana)==false);
                do{
                  cout << "Introduceti orientarea pentru P" << piesa.nume << " (H pentru orizontal, V pentru vertical, C pentru colt): ";
                cin >> orientare;
                if(orientare!='C'&& orientare!='H' && orientare!='V' )cout<<"NU PUTEM PUNE GARDUL IN ACEASTA POZITIE, VA RUGAM REINCERCATI";
                }while(orientare!='C'&& orientare!='H' && orientare!='V');

                
                if (orientare == 'H') {
                    if (coloana + piesa.latime > nrColoane-2) {
                        cout<<"UPS..IESE DIN MATRICE...REINCERCATI";
                    }
                     else{for (int i = 0; i <= piesa.latime; ++i) {
                        a[linie ][coloana  + i] = piesa.nume;
                    }
                    l++;afisareMatrice(a);
                     }
                } else if (orientare == 'V') {
                    if (linie + piesa.lungime > nrLinii-2) {
                        cout<<"UPS..IESE DIN MATRICE...REINCERCATI";
                    }
                     else{ for (int i = 0; i <= piesa.lungime; ++i) {
                        a[linie  + i][coloana ] = piesa.nume;
                    }
                    l++;afisareMatrice(a);
                       }
                }else if(orientare=='C')
                        {
                         if(coloana+piesa.latime > nrColoane-2 && linie + piesa.lungime > nrLinii-2)
                           cout<<"UPS..IESE DIN MATRICE...REINCERCATI";
                           else{ for (int i = 0; i <= piesa.lungime; ++i) {
                                         a[linie+i ][coloana] = piesa.nume;
                                                                  } 
                                     for (int i = 0; i <= piesa.latime; ++i) {
                                    a[linie][coloana+i] = piesa.nume;}
                                    l++;
                           }
                                                             
                          } 
                }

                
                if(l==3){cout << "Matrice dupa asezare:" << endl;
                afisareMatrice(a);
                }
    
                
    
               

            
            
        
  
  
     
    return 0;
}
