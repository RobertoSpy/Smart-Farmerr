void bordarematrice()
{
    for (int i = 0; i < 7; i++)
        for (int j = 0; j < 9; j++)
        {
            a[0][j] = -1;
            a[i][0] = -1;
            a[6][j] = -1;
            a[i][8] = -1;
        }
}

