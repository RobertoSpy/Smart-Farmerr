bool ok(int i, int j)
{
 if((i<1 || i>5 || j<1 || j>8) && (matrice[i][j]!=int('C')|| matrice[i][j]!=int('V')||matrice[i][j]!=int('O')||matrice[i][j]!=int('P')))
 return false;
 return true;
}
